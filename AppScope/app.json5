{
  "app": {
    "bundleName": "com.example.firsttest",
    "vendor": "example", // 标识对应用开发厂商的描述，
    "versionCode": 1000000,
    "versionName": "1.0.0",
    "icon": "$media:app_icon",
    "label": "$string:app_name", // 标识应用的名称，取值为字符串资源的索引，字符串长度不超过63字节。
    // "debug": true, 由IDE编译构建时生成。该标签可缺省，缺省值为false
    // "bundleType": "app" 标识应用的Bundle类型，用于区分应用或者元服务。
    "description" : "这是一个测试应用",
    "appEnvironments" : [ // 标识当前模块配置的应用环境变量
      {
        "name":"name1",
        "value": "value1"
      }
    ]
  }
}
